<h1>Put a new Representative on blast!</h1>

<%= form_for @representative do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br />
  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br />
  <%= f.label :email %>
  <%= f.text_field :email %><br />
  <%= f.label :office %>
  <%= f.text_field :office %><br />
  <%= f.label :party %>
  <%= f.text_field :party %><br />
  <%= f.label :elected %>
  <%= f.text_field :elected %><br />
  <%= f.label :bio %>
  <%= f.text_field :bio %><br />
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %><br />
  <%= f.label :picture_url %>
  <%= f.text_field :picture_url %><br />
  <%= f.submit "Add Representative" %>
<% end %>
